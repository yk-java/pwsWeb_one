			<property name="suppressAllComments" value="true" />
		</commentGenerator>
		<!-- jdbc连接信息 -->
		<jdbcConnection driverClass="com.mysql.jdbc.Driver"
			connectionURL="jdbc:mysql://10.0.0.28:3306/pwcloudos_dev" userId="root"
			password="g_lenS123" />
		<javaTypeResolver >  
			<property name="forces" value="false" />  
		</javaTypeResolver> 
		
		<!-- 生成对象 -->
		<javaModelGenerator targetPackage="com.glens.pwCloudOs.cj.base.vo"
			targetProject="pwsWeb">
			<property name="enableSubPackages" value="true" />
			<property name="trimStrings" value="true" />
		</javaModelGenerator>
		<!-- sqlMapper XML -->
		<sqlMapGenerator targetPackage="com.glens.pwCloudOs.cj.base.dao"
			targetProject="pwsWeb">
			<property name="enableSubPackages" value="true" />
